#include <iostream>

void testfun(int a, int b);

int main(int argc, char *argv[])
{
    int a,b;
    std::cin >> a >> b;
    
    testfun(a,b);
    return 0;
}


void testfun(int a, int b){
    int c = a-b > 0 ? 1:-1;
    std::cout << std::endl;
    std::cout << c << std::endl;
    c = a-b/b-a;
    std::cout << c << std::endl;
    std::cout << std::endl;
    for (int i = 0; i-1 != a-b ; i+=c)
    {
        std::cout << a-i << std::endl;
    }
}
